<div id="reqfood_body">
    <h2>Available : </h2>
    <div id="available">
        <% data.forEach((item)=>{%>
            <div class="availcard">
                <h4>Type : <%= item.ftype %></h4>
                <h4> Quality : <%= item.quality %></h4>
                <p>Available(kg) : <%= item.quantity %></p>
                <p>Offered by <%= item.organisation %></p>
                <p>Contact : <%= item.contact %></p>
                <p>Placed on <%= item.time %></p>
                <p>Expires in <%= item.expiry %>hrs</p>
            </div>
        <%})%>
    </div>
</div>